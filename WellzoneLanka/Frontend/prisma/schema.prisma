generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGO")
}

model User {
  id            String   @id @default(auto()) @map("_id") @db.ObjectId
  name          String?   
  email         String?   @unique
  avatar        String?
  createAt      DateTime  @default(now())
  updateAt      DateTime  @updatedAt
}

